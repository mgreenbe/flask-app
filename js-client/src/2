DOMParser = require('xmldom').DOMParser;

const xml = `<div class="outer"><p>Solve the equation x + 2 = 3. x = <input path="x" /></p></div>`;
console.log(xml);

let parser = new DOMParser();
let doc = parser.parseFromString(xml);

console.log(doc.childNodes.length);
let div = doc.childNodes[0];
console.log(div.attributes[0].nodeName);
